#include <stdio.h>
#include <stdlib.h>

int main(void)
{
	char num1 = 'A' , num2 = 'B';

	printf("Calculadora\n");
	printf("1 - Ingresar primer operando: %c\n", num1);
	printf("2 - Ingresar segundo operando: %c\n", num2);
	printf("3 - Calcular todas las operaciones");




	return EXIT_SUCCESS;

}
